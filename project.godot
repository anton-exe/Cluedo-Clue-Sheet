config_version=4

[application]

config/name="Cluedo Clue Sheet"
config/description="An app to write down the different info you collect during a game of cluedo."
run/main_scene="res://MainMenu.tscn"
boot_splash/bg_color=Color( 0.0862745, 0.129412, 0.301961, 1 )
config/icon="res://icon.png"

[autoload]

